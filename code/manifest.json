{
  "manifest_version": 2,

  "name": "YSSY Upload Viewer",
  "description": "YSSY Upload Viewer",
  "version": "1.0.2",

  "permissions": [
    "https://bbs.sjtu.edu.cn/*"
  ],
  
  "icons" : {
  	"128": "icon.png"
  },
  
  "options_page" : "options.html",
  
  "content_scripts": [
    {
      "matches": ["https://bbs.sjtu.edu.cn/*"],
      "js": ["jquery-2.1.0.min.js", "options.js", "viewer.js"]
    }
  ]
}